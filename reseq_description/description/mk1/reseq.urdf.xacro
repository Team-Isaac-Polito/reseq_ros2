<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="reseq">

  <xacro:arg name="use_ros2_control" default="true"/>
  <xacro:arg name="sim_mode" default="false"/>

  <xacro:include filename="$(find reseq_description)/description/reseq_parameters.xacro" />
  
  <xacro:include filename="$(find reseq_description)/description/macros/inertial_macros.xacro" />
  <xacro:include filename="$(find reseq_description)/description/macros/utils.xacro" />

  <xacro:include filename="$(find reseq_description)/description/macros/reseq_body.xacro" />

  <xacro:include filename="$(find reseq_description)/description/arm_mk1/reseq_arm_mk1.xacro" />

  <xacro:include filename="$(find reseq_ros2)/description/ros2_control.xacro" />

</robot>
