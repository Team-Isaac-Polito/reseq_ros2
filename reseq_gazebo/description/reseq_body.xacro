<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- This is a parametric description of reseq with 4 modules -->

  <!-- BASE LINK (center of equivalent wheel of module 1)-->
  <link name="base_link" />

  <!-- CHASSIS1 -->
  <joint name="chassis1_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis1_link"/>
    <!-- leaving 1/3 of the EquivalentWheelDiameter from front (origin is in the center of eq wheel, at the bottom of the chassis) -->
    <origin xyz="0 0 ${(EquivalentWheelDiameter-2*ChassisTopAndBottomClearance)/2+ChassisTopAndBottomClearance}"/>
  </joint>

  <link name="chassis1_link">
    <visual>
      <origin xyz="${EquivalentWheelDiameter/12+(toll+JointDiameter/2)/2} 0 0"/>
      <geometry>
        <!-- leaving 1/3 of the EquivalentWheelDiameter from front and toll from Joint and Tracks (for clearance) -->
        <box size="${WheelBase-JointDiameter/2-toll+EquivalentWheelDiameter*2/3-EquivalentWheelDiameter/2} ${ModuleWidth-2*TrackWidth-2*toll} ${EquivalentWheelDiameter-2*ChassisTopAndBottomClearance}"/>
      </geometry>
      <material name="ChassisColor"/>
    </visual>
    
    <collision>
      <origin xyz="${EquivalentWheelDiameter/12+(toll+JointDiameter/2)/2} 0 0"/>
      <geometry>
        <!-- leaving 1/3 of the EquivalentWheelDiameter from front and toll from Joint and Tracks (for clearance) -->
        <box size="${WheelBase-JointDiameter/2-toll+EquivalentWheelDiameter*2/3-EquivalentWheelDiameter/2} ${ModuleWidth-2*TrackWidth-2*toll} ${EquivalentWheelDiameter-2*ChassisTopAndBottomClearance}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_box mass="${ChassisMass}" x="${WheelBase-JointDiameter/2-toll+EquivalentWheelDiameter*2/3-EquivalentWheelDiameter/2}" y="${ModuleWidth-2*TrackWidth-2*toll}" z="${EquivalentWheelDiameter-2*ChassisTopAndBottomClearance}">
      <origin xyz="${EquivalentWheelDiameter/12+(toll+JointDiameter/2)/2} 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>

  <gazebo reference="chassis1_link">
    <material>Gazebo/White</material>
  </gazebo>

  <!-- LEFT FRONT WHEEL 1-->
  <joint name="left_front_wheel_1_joint" type="continuous">
    <parent link="chassis1_link"/>
    <child link="left_front_wheel_1_link"/>
    <origin xyz="${WheelBase/2} ${ModuleWidth/2-TrackWidth/2} 0" rpy="-${pi/2} 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_front_wheel_1_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
      <material name="WheelColor"/>
    </visual>
    
    <collision>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_front_wheel_1_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- RIGHT FRONT WHEEL 1-->
  <joint name="right_front_wheel_1_joint" type="continuous">
    <parent link="chassis1_link"/>
    <child link="right_front_wheel_1_link"/>
    <origin xyz="${WheelBase/2} ${-ModuleWidth/2+TrackWidth/2} 0" rpy="${pi/2} 0 0" />
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="right_front_wheel_1_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
      <material name="WheelColor"/>
    </visual>
    
    <collision>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_front_wheel_1_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- LEFT BACK WHEEL 1-->
  <joint name="left_back_wheel_1_joint" type="continuous">
    <parent link="chassis1_link"/>
    <child link="left_back_wheel_1_link"/>
    <origin xyz="${-WheelBase/2} ${ModuleWidth/2-TrackWidth/2} 0" rpy="-${pi/2} 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_back_wheel_1_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
      <material name="WheelColor"/>
    </visual>
    
    <collision>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_back_wheel_1_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- RIGHT BACK WHEEL 1-->
  <joint name="right_back_wheel_1_joint" type="continuous">
    <parent link="chassis1_link"/>
    <child link="right_back_wheel_1_link"/>
    <origin xyz="${-WheelBase/2} ${-ModuleWidth/2+TrackWidth/2} 0" rpy="${pi/2} 0 0" />
    <axis xyz="0 0 -1"/>
  </joint>
  
  <link name="right_back_wheel_1_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
      <material name="WheelColor"/>
    </visual>
    
    <collision>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_back_wheel_1_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- JointA 1-->
  <joint name="joint_a_1_joint" type="revolute">
    <parent link="chassis1_link"/>
    <child link="joint_a_1_link"/>
    <!-- Effort in Nm -->
    <limit effort="20.0" lower="${-pitch}" upper="${pitch}" velocity="0.5"/>
    <origin xyz="${-WheelBase/2} 0 0" rpy="${pi/2} 0 0" />
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="joint_a_1_link">
    <!-- gearbox cilinder -->
    <visual>
      <geometry>
        <cylinder radius="${JointDiameter/2}" length="${ModuleWidth-2*TrackWidth-toll*2}"/>
      </geometry>
      <material name="JointColor"/>
    </visual>
    <!-- horizontal shaft -->
    <visual>
      <origin xyz="${-JointA/2} 0 0" />
      <geometry>
        <box size="${JointA} ${EquivalentWheelDiameter-2*JointABottomClearance} ${ModuleWidth-2*TrackWidth-toll*2}"/>
      </geometry>
      <material name="JointColor"/>
    </visual>
    <!-- to hide space wile rotating -->
    <visual>
      <origin xyz="${-JointA} 0 0" />
      <geometry>
        <sphere radius="${(ModuleWidth-2*TrackWidth-toll*2)/2}"/>
      </geometry>
      <material name="JointColor"/>
    </visual>

    <!-- gearbox cilinder -->
    <collision>
      <geometry>
        <cylinder radius="${JointDiameter/2}" length="${ModuleWidth-2*TrackWidth-toll*2}"/>
      </geometry>
    </collision>
    <!-- horizontal shaft -->
    <collision>
      <origin xyz="${-JointA/2} 0 0" />
      <geometry>
        <box size="${JointA} ${EquivalentWheelDiameter-2*JointABottomClearance} ${ModuleWidth-2*TrackWidth-toll*2}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_box mass="${JointAMass}" x="${JointA}" y="${EquivalentWheelDiameter-2*JointABottomClearance}" z="${ModuleWidth-2*TrackWidth-toll*2}">
      <origin xyz="${-JointA/2} 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>

  <gazebo reference="joint_a_1_link">
    <material>Gazebo/Grey</material>
  </gazebo>

  <!-- JointB 1-->
  <joint name="joint_b_1_joint" type="revolute">
    <parent link="joint_a_1_link"/>
    <child link="joint_b_1_link"/>
    <!-- Effort in Nm -->
    <limit effort="20.0" lower="${-yaw}" upper="${yaw}" velocity="0.5"/>
    <origin xyz="${-JointA} 0 0" rpy="0 ${pi/2} 0" />
    <axis xyz="0 1 0"/>
  </joint>

  <link name="joint_b_1_link">
    <visual>
      <origin xyz="0 0 ${-(JointB-WheelBase/2-EquivalentWheelDiameter/6)/2}" />
      <geometry>
        <cylinder radius="${(ModuleWidth-2*TrackWidth-toll*2)/2}" length="${JointB-WheelBase/2-EquivalentWheelDiameter/6}"/>
      </geometry>
      <material name="JointColor"/>
    </visual>

    <xacro:inertial_cylinder mass="${JointBMass}" length="${JointB-WheelBase/2-EquivalentWheelDiameter/6}" radius="${(ModuleWidth-2*TrackWidth-toll*2)/2}">
      <origin xyz="0 0 ${-(JointB-WheelBase/2-EquivalentWheelDiameter/6)/2}" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="joint_b_1_link">
    <material>Gazebo/Grey</material>
  </gazebo>

  <!-- CHASSIS2 -->
  <joint name="chassis2_joint" type="revolute">
    <parent link="joint_b_1_link"/>
    <child link="chassis2_link"/>
    <!-- Effort in Nm -->
    <limit effort="2.0" lower="${-roll}" upper="${roll}" velocity="0.5"/>
    <!-- leaving 1/3 of the EquivalentWheelDiameter from front (origin is in the center of eq wheel, at the bottom of the chassis) -->
    <origin xyz="0 0 ${-JointB}" rpy="${-pi/2} ${-pi/2} 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <link name="chassis2_link">
    <visual>
      <origin xyz="${EquivalentWheelDiameter/12+(toll+JointDiameter/2)/2} 0 0"/>
      <geometry>
        <!-- leaving 1/3 of the EquivalentWheelDiameter from front and toll from Joint and Tracks (for clearance) -->
