    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_front_wheel_2_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- RIGHT FRONT WHEEL 2-->
  <joint name="right_front_wheel_2_joint" type="continuous">
    <parent link="chassis2_link"/>
    <child link="right_front_wheel_2_link"/>
    <origin xyz="${WheelBase/2} ${-ModuleWidth/2+TrackWidth/2} 0" rpy="${pi/2} 0 0" />
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="right_front_wheel_2_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
      <material name="WheelColor"/>
    </visual>
    
    <collision>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
      </geometry>
    </collision>
    
    <xacro:inertial_cylinder mass="${WheelMass}" length="${TrackWidth}" radius="${EquivalentWheelDiameter/2}">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_front_wheel_2_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- LEFT BACK WHEEL 2-->
  <joint name="left_back_wheel_2_joint" type="continuous">
    <parent link="chassis2_link"/>
    <child link="left_back_wheel_2_link"/>
    <origin xyz="${-WheelBase/2} ${ModuleWidth/2-TrackWidth/2} 0" rpy="-${pi/2} 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_back_wheel_2_link">
    <visual>
      <geometry>
        <cylinder radius="${EquivalentWheelDiameter/2}" length="${TrackWidth}"/>
