    <material>Gazebo/Black</material>
  </gazebo>

  <!-- LEFT BACK WHEEL 4-->
  <joint name="left_back_wheel_4_joint" type="continuous">
    <parent link="chassis4_link"/>
    <child link="left_back_wheel_4_link"/>
    <origin xyz="${-WheelBase/2} ${ModuleWidth/2-TrackWidth/2} 0" rpy="-${pi/2} 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_back_wheel_4_link">
