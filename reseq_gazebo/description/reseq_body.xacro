    <parent link="chassis3_link"/>
    <child link="left_front_wheel_3_link"/>
    <origin xyz="${WheelBase/2} ${ModuleWidth/2-TrackWidth/2} 0" rpy="-${pi/2} 0 0" />
    <axis xyz="0 0 1"/>
  </joint>

  <link name="left_front_wheel_3_link">
