    <material>Gazebo/Black</material>
  </gazebo>

  <!-- RIGHT FRONT WHEEL 4-->
  <joint name="right_front_wheel_4_joint" type="continuous">
    <parent link="chassis4_link"/>
    <child link="right_front_wheel_4_link"/>
    <origin xyz="${WheelBase/2} ${-ModuleWidth/2+TrackWidth/2} 0" rpy="${pi/2} 0 0" />
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="right_front_wheel_4_link">
