            </command_interface> 
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="left_back_wheel_3_joint"> 
            <command_interface name="velocity">
                <param name="min">${-WheelSpeed}</param>
                <param name="max">${WheelSpeed}</param>
            </command_interface> 
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>
        <joint name="right_back_wheel_3_joint"> 
            <command_interface name="velocity">
                <param name="min">${-WheelSpeed}</param>
                <param name="max">${WheelSpeed}</param>
            </command_interface> 
            <state_interface name="position"/>
            <state_interface name="velocity"/>
        </joint>

        <joint name="joint_a_3_joint">
