        </joint>

        <joint name="arm_mK1_joint">
            <command_interface name="position"/>
            <state_interface name="position"/>
            <state_interface name="effort"/>
        </joint>

    </ros2_control> 

    <gazebo>
        <plugin name="gazebo_ros2_control" filename="libgazebo_ros2_control.so">
            <parameters>$(find reseq_gazebo)/config/my_controllers.yaml</parameters>
            <parameters>$(find reseq_gazebo)/config/gaz_ros2_ctl_use_sim.yaml</parameters>
        </plugin>
    </gazebo>

</robot>
